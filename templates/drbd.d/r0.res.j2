# {{ ansible_managed }}
resource r0 {
  device /dev/drbd0;
  disk /dev/sdb1;
  meta-disk internal;
  on virt-cl-drbd-0 {
     address {{ hostvars['virt-cl-drbd-0']['enp2s0f1']['address'] }}:7780;
  }
  on virt-cl-drbd-1 {
     address {{ hostvars['virt-cl-drbd-1']['enp2s0f1']['address'] }}:7780;
  }
}
